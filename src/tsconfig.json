{
  "compilerOptions": {
    "outDir": "dist",
    "moduleResolution": "node",
    "sourceMap": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "module": "commonjs",
    "target": "es5",
    "suppressImplicitAnyIndexErrors": true,
    "allowJs": true,
    "jsx": "react",
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}